{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "quarto",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "definition"
      }
    },
    "definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_markdownBlock"
        }
      ]
    },
    "_markdownBlock": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "startBlock"
        },
        {
          "type": "SYMBOL",
          "name": "languageId"
        },
        {
          "type": "SYMBOL",
          "name": "startBlock"
        }
      ]
    },
    "startBlock": {
      "type": "STRING",
      "value": "```"
    },
    "languageId": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "."
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}