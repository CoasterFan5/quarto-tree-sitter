{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "quarto",
  "rules": {
    "hello": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_def"
      }
    },
    "_def": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_blockDef"
        }
      ]
    },
    "_blockDef": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "STRING",
          "value": "```"
        }
      ]
    },
    "_md": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "##"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "."
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}